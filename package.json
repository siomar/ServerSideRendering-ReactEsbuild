{
  "name": "unova",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon  --experimental-modules ./scripts/dev"
  },
  "dependencies": {
    "babel-preset-react-app": "10.0.0",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/register": "^7.18.6",
    "@types/express": "^4.17.13",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "esbuild": "^0.14.48",
    "nodemon": "^2.0.18"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "nodemonConfig": {
    "verbose": false,
    "watch": [
      "."
    ],
    "ext": "js,jsx,json,css",
    "ignore": [
      "build/**/*",
      "server/render.js",
      "node_modules/**/*"
    ]
  }
}
